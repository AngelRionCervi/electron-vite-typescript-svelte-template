{
  "name": "electron-vite-svelte",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "start": "electron .",
    "dev:electron": "concurrently \"tsc -w --project ./src/main/tsconfig.json\" \"cross-env NODE_ENV=dev  electron .\"",
    "dev:vite": "vite",
    "dev:all": "concurrently -n=svelte,electron -c=yellow,blue \"npm run dev:vite\" \"npm run dev:electron\"",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@tsconfig/svelte": "^5.0.2",
    "electron": "^28.1.0",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3"
  }
}
